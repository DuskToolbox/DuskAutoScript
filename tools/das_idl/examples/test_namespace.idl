// DAS IDL 测试文件 - 演示 namespace 支持
// 测试多个命名空间和嵌套接口

// 全局枚举（无命名空间）
enum GlobalStatus {
    Success = 0,
    Failed = -1,
    Pending = 1,
}

// Das 命名空间中的枚举
namespace Das {
    enum TestLevel {
        Basic = 0,
        Advanced = 1,
        Expert = 2,
    }

    [uuid("a1111111-1111-1111-1111-111111111111")]
    interface IDasTestBase : IDasBase {
        DasResult GetName([out] DasReadOnlyString** pp_out_name);
        [get] int32 Id
    }

    [uuid("a2222222-2222-2222-2222-222222222222")]
    interface IDasTestResult : IDasTestBase {
        [get, set] DasTestLevel Level
        DasResult GetMessage([out] DasReadOnlyString** pp_out_message);
    }
}

// 另一个命名空间
namespace Utils {
    enum ErrorCode {
        NoError = 0,
        IOError = -1,
        MemoryError = -2,
    }

    [uuid("b1111111-1111-1111-1111-111111111111")]
    interface IUtilsLogger : IDasBase {
        DasResult Log(DasReadOnlyString message);
        [get, set] bool EnableDebug
    }
}
